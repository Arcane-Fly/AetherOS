{
  "$schema": "https://schema.railpack.com",
  "version": "1",
  "metadata": {
    "name": "aetheros-main"
  },
  "build": {
    "provider": "node",
    "steps": {
      "install": {
        "commands": ["yarn install --frozen-lockfile"]
      },
      "build": {
        "commands": ["yarn workspace aetheros-frontend build"]
      }
    }
  },
  "deploy": {
    "startCommand": "yarn workspace aetheros-frontend start",
    "healthCheckPath": "/",
    "healthCheckTimeout": 300
  }
}