{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://aetheros.dev/contracts/browserbase/navigate.response.json",
  "title": "browserbase.navigate.response",
  "description": "Response schema for browser navigation operations",
  "type": "object",
  "additionalProperties": false,
  "required": ["success", "url"],
  "properties": {
    "success": {
      "type": "boolean",
      "description": "Whether navigation completed successfully"
    },
    "url": {
      "type": "string",
      "format": "uri",
      "description": "Final URL after navigation (may differ due to redirects)"
    },
    "title": {
      "type": "string",
      "description": "Page title"
    },
    "statusCode": {
      "type": "integer",
      "minimum": 100,
      "maximum": 599,
      "description": "HTTP status code of the response"
    },
    "loadTime": {
      "type": "number",
      "minimum": 0,
      "description": "Page load time in milliseconds"
    }
  }
}
