  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "express-winston": "^4.2.0",
    "helmet": "^6.1.5",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.0",
    "supertest": "^6.3.0"
  },
  "packageManager": "yarn@4.9.2"
