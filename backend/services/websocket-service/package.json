{
  "name": "aetheros-websocket-service",
  "version": "1.0.0",
  "description": "WebSocket service for real-time collaboration in AetherOS",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js --max-warnings 0",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "socket.io": "^4.7.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.0",
    "redis": "^4.6.5",
    "helmet": "^6.1.5",
    "express-rate-limit": "^6.7.0",
    "dotenv": "^16.0.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.5.0",
    "supertest": "^6.3.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "websocket",
    "real-time",
    "collaboration",
    "aetheros"
