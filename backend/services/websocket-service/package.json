{
  "name": "aetheros-websocket-service",
  "version": "1.0.0",
  "description": "WebSocket service for real-time collaboration in AetherOS",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js --max-warnings 0",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "winston": "^3.18.1"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "websocket",
    "real-time",
    "collaboration",
    "aetheros"
  ],
  "author": "AetherOS Team",
  "license": "MIT",
  "packageManager": "yarn@4.9.2"
}
