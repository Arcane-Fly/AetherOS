{
  "$schema": "https://schema.railpack.com",
  "version": "1",
  "metadata": {
    "name": "aetheros-websocket-service"
  },
  "build": {
    "provider": "node",
    "steps": {
      "install": {
        "commands": ["yarn install --immutable"]
      }
    }
  },
  "deploy": {
    "startCommand": "yarn start",
    "healthCheckPath": "/health",
    "healthCheckTimeout": 300
  }
}